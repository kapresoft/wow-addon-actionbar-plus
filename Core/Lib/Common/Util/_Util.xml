<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

    <Script file="IdleTracker.lua"/>
    <Frame parentKey="IdleTracker" parent="ActionbarPlusEventFrame">
        <Scripts>
            <OnLoad>ABP_XML:IdleTracker_OnLoad(self)</OnLoad>
            <OnEvent>ABP_XML:IdleTracker_OnEvent(self, event)</OnEvent>
            <OnUpdate>ABP_XML:IdleTracker_OnUpdate(self, elapsed)</OnUpdate>
        </Scripts>
    </Frame>

    <Script file="KeybindTextUtil.lua"/>
    <Script file="CooldownUtil.lua"/>
    <Script file="ItemUtil.lua"/>
    <Script file="MacroUtil.lua"/>
    <Script file="RangeIndicatorUtil.lua"/>
    <Script file="TooltipUtil.lua"/>

    <Script file="ThrottledUpdaterMixin.lua"/>
    <Frame parentKey="ThrottledUpdaterMixinDriver" parent="ActionbarPlusEventFrame">
        <Scripts>
            <OnLoad>ABP_XML:ThrottledUpdaterMixin_OnLoad(self)</OnLoad>
            <OnUpdate>ABP_XML:ThrottledUpdaterMixin_OnUpdate(self, elapsed)</OnUpdate>
        </Scripts>
    </Frame>

</Ui>
